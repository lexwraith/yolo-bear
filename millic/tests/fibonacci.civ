number main()
{
   number n, first = 0, second = 1, next, c;
 
   print("Enter the number of terms\n");
   scanf("%d",&n);
 
   print("First %d terms of Fibonacci series are :-\n",n);
 
   for ( c = 0 ; c < n ; c++ )
   {
      if ( c <= 1 )
         next = c;
      else
      {
         next = first + second;
         first = second;
         second = next;
      }
      print("%d\n",next);
   }
 
   return 0;
}