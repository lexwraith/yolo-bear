number main()
{
   number n, sum = 0, c, value;
 
   print("Enter the number of integers you want to add\n");
   scanf("%d", &n);
 
   print("Enter %d integers\n",n);
 
   for (c = 1; c <= n; c++)
   {
      scanf("%d",&value);
      sum = sum + value;
   }
 
   print("Sum of entered integers = %d\n",sum);
 
   return 0;
}
